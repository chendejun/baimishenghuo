/* 首页 */
.index_box{padding: 20px 30px; background: #fff;}
.index_box .block_box{margin: 20px 0;}
.index_box  h4{ font-size: 14px; color: #333; line-height: 30px;}
.index_box .title{position: relative;height: 40px; margin-bottom: 10px;}
.type_row{height: 40px; line-height: 40px; position: absolute; top: 0; right: 0;color: #666;}
.type_row .bm-input{ height: 38px; line-height: 38px;}
.type_row .bm-btn{ height: 40px; line-height: 40px;}
.menu_box{ padding: 20px 0;}
.menu_box .mpart li{ width: 33.3%; float: left; text-align: center;position: relative;}
.menu_box .mpart li .mod_info{display: inline-block;padding-left: 20px;vertical-align: top;position: relative;}
.menu_box .mpart li .mod_info p{color: #999;line-height: 30px; padding-bottom: 10px; margin: 0;}
.menu_box .mpart li .mod_info span{color: #f56c6c;line-height: 40px; font-size: 36px;}
.menu_box .mpart li .mod_info b{color: #bbb;font-size: 13px; font-weight: normal;}
.menu_box .mpart li:after{ content:''; width: 1px; height: 100%; background: #f2f2f2; position: absolute; top: 0; right: 0;}
.menu_box .mpart li:last-child:after{width: 0;}
i.tips{width:15px;height:15px;background: url(../../public/img/tip_tp.png) no-repeat;display: inline-block;vertical-align: middle;margin-top: -2px;margin-left: 6px;cursor: pointer;}
.tips_btxt{color: #999;font-size: 14px;line-height:22px;border: 1px solid #d4d4d4;padding:10px;border-radius:6px;width: 270px;text-align: left; position: absolute;top: -100px;right: -223px;background:#fff;}
.tips_btxt:before{position: absolute;content: ""; width: 0;height: 0; left: 45px; bottom: -8px;border-top: 8px solid #d4d4d4;border-right: 8px solid transparent; border-left: 8px solid transparent;}
.tips_btxt:after{position: absolute;content: "";width: 0;height: 0; left: 46px;bottom: -7px; border-top: 7px solid #fff; border-right: 7px solid transparent; border-left: 7px solid transparent;}

/* 我的广告主 */
.advertr_box{padding: 20px 30px; background: #fff; position: relative;}
.advertr_box .btn_link{line-height: 40px; height: 40px; float: right;}
.advertr_box .set_main_ad{height: 73px; width: 100%;}
.advertr_box .ipt_box { width: 410px; float: left;}
.advertr_box .ipt_box input{ height: 38px; line-height: 38px;display: inline-block;vertical-align: middle;}
.advertr_box .ipt_box select{ height: 40px; line-height: 40px;display: inline-block;vertical-align: middle;}
.advertr_box td i{ font-size: 0;}
.advertr_box td .table-option-btn{ text-decoration:none;}
.ul_box{padding: 30px 0 10px;}
.ul_box ul li{ width: 33.3%; float: left;position: relative;}
.ul_box ul li:after{ content:''; width: 1px; height: 100%; background: #f2f2f2; position: absolute; top: 0; right: 0;}
.ul_box ul li:last-child:after{width: 0;}
.ul_box ul li p{font-size:14px; color: #999; padding-left: 100px; line-height: 30px;}
.ul_box ul li span{font-size:36px; color: #f56c6c; line-height: 44px; display: block; text-align: center;}
.notice_box{position: absolute;top: 20px;right: 30px;height: 30px;overflow: hidden;line-height: 30px;width: 300px; cursor: pointer;}
.notice_box i{background: url(../../public/img/notice.png) no-repeat center; width: 30px;height: 30px; display: inline-block; vertical-align: top;}
.notice_list {display:inline-block;}
.notice_list li{color: #999; font-size: 14px;}
.delivery_mode {width: 100%;height: 100%;text-align: center;padding-bottom: 23px;}
.delivery_mode .bluecolor{color: #5877a4 !important;}
.delivery_mode>h3 {font-size: 16px;color: #333;height: 62px;line-height: 62px;font-weight: 900;margin: 0;}
.delivery_mode .ad_type {height: 44px;line-height: 44px;font-size: 14px;display: block;color: #333;}

