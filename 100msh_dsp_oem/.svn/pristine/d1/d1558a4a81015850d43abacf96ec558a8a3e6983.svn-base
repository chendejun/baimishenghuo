                    left: 'left',
                    textStyle: {
                        color: '#333',
                        fontStyle: 'normal',
                        fontWeight: 'bold',
                        fontSize: 14
                    }
                },
                tooltip: {
                    trigger: 'item',
                    formatter: "{a} <br/>{b}: {c} ({d}%)",
                },
                series: [{
                    name: text,
                    type: 'pie',
                    radius: ['42%', '55%'],
                    color: ['#53d99d', '#f6cb21', '#618cff', '#fc706f', '#fecbd4'],
                    label: {
                        normal: {
                            formatter: '{b}\n{d}%'
                        },
                    },
                    data: data
                }]
            };
            el.setOption(set);
