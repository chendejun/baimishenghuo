    	</script>
		<script type="text/javascript" src="../../../../../public/js/jquery-1.8.3.min.js"></script>
		<script type="text/javascript" src="../../../../../public/js/common.js"></script>
		<script src="../../../../../public/js/layer/layer.js"></script>
		<script src="../../../../../public/js/laypage/laypage.js"></script>
		<script type="text/javascript" src="../../../../../public/js/laytpl/laytpl.js"></script>

		<script type="text/javascript">
			//sessionStorage.clear();//清除存储数据
			laytpl.config({ open: '<<', close: '>>' }); //laytpl与php冲突 标签修改
			var navType = 1,
				keywords = "";
			//平台数据切换
			function SetNavStyle() {
				$('.ad_nav').find('.nav').on({
					click: function () {
						$(this).addClass('nav_active').siblings().removeClass('nav_active');
						navType = $('.nav_active').attr('data-type');
						
						if(navType == 1 || navType == 2){
							getList();
						}else if(navType == 3){
							getCampaignList();
						}
					}
				})
			}
			SetNavStyle()
			//搜索
			function searchFn() {
				navType = $('.nav_active').attr('data-type');
				keywords = $.trim($('#s_material').val());
				if(navType == 1 || navType == 2){
					getList();
				}else if(navType == 3){
					getCampaignList();
				}
			}
			//获取数据
			var limit = 10;
			function getList(curr) {
				if (keywords == "") {
					var url = '/v1.2/Gdtdelivery/index/page/' + (curr || 1) + '/limit/' + limit + '/platform_id/' + navType
				} else {
					var url = '/v1.2/Gdtdelivery/index/page/' + (curr || 1) + '/limit/' + limit + '/platform_id/' + navType + '/name/' + keywords
				}
				$.ajax({
					type: "GET",
					url: url,
					dataType: 'json',
					success: function (rs) {
						tipTopHide();       //隐藏提示框
						console.log(rs)
						if (rs.state == 1) {         //如果查询成功
							var data = rs.data;     //返回数据赋值
							render_table(data);     //渲染表格数据					
							renderPage(data.total_page, curr);
						} else {
							tipTopShow(rs.msg);
						}
					},
					error: function (rs) {
						tipTopShow("查询信息失败，请重试...");    //显示提示框
					}
				});
			}
//			获取广告计划列表
            function getCampaignList(curr) {
				$.ajax({
					type: "POST",
					url: '/v1.2/Bhdelivery/getCampaignList',
					dataType: 'json',
					data:{
						name:keywords,
						page:curr || 1
					},
					success: function (rs) {
						tipTopHide();       //隐藏提示框
						console.log(rs)
						if (rs.api_code == 1) {         //如果查询成功
							var data = rs.data;     //返回数据赋值
							render_table(data);     //渲染表格数据					
							renderPage1(data.page_total, curr);
						} else {
							tipTopShow(rs.msg);
						}
					},
					error: function (rs) {
						tipTopShow("查询信息失败，请重试...");    //显示提示框
					}
				});
			}



			//渲染投放计划列表（一级）
			var delivery_status = { '0': '待审核', '1': '投放中', '2': '审核失败', '3': '投放结束', '4': '暂停' },
				configured_status={'1':'有效','2':'暂停','3':'结束'},
				bid_type = { '1': 'CPM', '2': 'CPC' };
			function render_table(data) {
				var list_e = $("#plan_list tbody"), str = "", no_msg = $(".noMsg");
				if (data.list.length == 0) {
					list_e.html("");
					no_msg.text("暂无记录");
					return;
				}
				no_msg.text("");
				$.each(data.list, function (k, v) {
					var stateText = "",   //投放状态
						adName = "",  //广告名称
						platform_name = "",   //平台类型
						openList = "",   //查看列表
						dayMoney = "";   //日预算
						createTime = '';//创建日期
						cpc_num = '';//点击量
						cpm_num='';//曝光量
						reportspend = '';//花费
					//腾讯广告  2   wifi广告   1
					if (navType == 1) {
						adName = '<a href="javascript:;" _id="' + v.delivery_id + '" class="table-option-btn view_info">' + v.delivery_name + '</a>'  //投放名称
						//广告投放状态
						stateText = delivery_status[v.delivery_status];
						platform_name = v.platform_name;
						openList = '<a href="javascript:;" _id="' + v.delivery_id + '" class="table-option-btn" onclick="ChartsDetails(' + v.delivery_id + ","+0+')">查看详情</a>'
						day_money = v.day_money;
						createTime = v.create_time;
						cpc_num = v.z_cpc_num;
						cpm_num = v.z_cpm_num;
						reportspend = round(v.ad_pay, 2);
					} else if (navType == 2) {
						adName = '<a href="javascript:;" _id="' + v.campaign_id + '" class="table-option-btn view_list" onclick="getDevertiserData(' + v.campaign_id + "," + v.configured_status + "," + k + ')">' + v.campaign_name + '</a>'  //计划名称
						//腾讯的计划投放状态
						stateText = configured_status[v.configured_status];
						openList = '<a href="javascript:;" _id="' + v.campaign_id + '" class="table-option-btn" onclick="getDevertiserData(' + v.campaign_id + "," + v.configured_status + "," + k + ')">查看列表</a>'
						day_money = v.daily_budget / 100;
						createTime = v.create_time;
						cpc_num = v.z_cpc_num;
						cpm_num = v.z_cpm_num;
						reportspend = round(v.ad_pay, 2);
					}else if(navType == 3){
						adName = '<a href="javascript:;" _id="' + v.bh_campaign_id + '" class="table-option-btn view_list" onclick="getDeliveryList(' + v.bh_campaign_id + "," + k + ')">' + v.bh_campaign_name + '</a>'  //计划名称
						//全网计划投放状态
						stateText = v.status_name;
						openList = '<a href="javascript:;" _id="' + v.bh_campaign_id + '" class="table-option-btn" onclick="getDeliveryList(' + v.bh_campaign_id + "," + k +')">查看列表</a>'
						day_money = v.budget;
						createTime = v.add_time.slice(0,10);
						cpc_num = v.cpc_num;
						cpm_num = v.cpm_num;
						reportspend = v.reportspend;
					}
					if ($('.nav_active').attr('data-type') == 1) {
						platform_name = 'WIFI广告';
					} else if ($('.nav_active').attr('data-type') == 2) {
						platform_name = '腾讯广告';
					} else if ($('.nav_active').attr('data-type') == 3) {
						platform_name = '全网广告';
					}
					str += '<tr class="plan' + k + '">'
					str += '<td>'
					str += adName
					str += '</td>'
					str += '<td>' + platform_name + '</td>'   //投放平台
					str += '<td>' + v.comp_name + '</td>'   //广告主
					str += '<td>' + createTime + '</td>'   //申请日期
					str += '<td>' + stateText + '</td>'
					str += '<td>' + day_money + '</td>'
					str += '<td>' + cpm_num + '</td>'
					str += '<td>' + cpc_num + '</td>'
					str += '<td>' + v.clicking_rate + '</td>'
					str += '<td>￥' + reportspend + '</td>'
					str += '<td>' + openList + '</td>'
					str += '</tr>'
				});
				list_e.html(str);
			}
			//广告投放列表（二级）
			function getDevertiserData(id, status, parentId) {
				var TrNode = $(".plan" + parentId);
				if (TrNode.hasClass('active') == false) {
					TrNode.addClass('active');
					TrNode.children().last().html('<a href="javascript:;" _id="' + id + '" class="table-option-btn" onclick="getDevertiserData(' + id + "," + status + "," + parentId + ')">收起列表</a>')
					$.ajax({
						type: "GET",
						url: '/v1.2/Gdtdelivery/getgdtAdList/campaign_id/' + id + '/camp_status/' + status,
						dataType: 'json',
						success: function (rs) {
							tipTopHide();       //隐藏提示框
							console.log(rs)
							if (rs.state == 1) {         //如果查询成功
								var data = rs.data;     //返回数据赋值
								if (data.length == 0) {
									tipTopShow('暂无投放记录！');
									TrNode.removeClass('active');
									TrNode.children().last().html('<a href="javascript:;" _id="' + id + '" class="table-option-btn" onclick="getDevertiserData(' + id + "," + status + "," + parentId + ')">查看列表</a>')
									return;
								} else {
									renderadList(data, TrNode);     //渲染表格数据		
								}
							} else {
								tipTopShow(rs.msg);
							}
						},
						error: function (rs) {
							tipTopShow("查询信息失败，请重试...");    //显示提示框
						}
					});
				} else {
					TrNode.removeClass('active');
					TrNode.next('.inner_table').remove();
					TrNode.children().last().html('<a href="javascript:;" _id="' + id + '" class="table-option-btn" onclick="getDevertiserData(' + id + "," + status + "," + parentId + ')">查看列表</a>');
				}

			}
			
//			获取全网广告投放列表
			function getDeliveryList(id,parentId){
				var TrNode = $(".plan" + parentId);
				if (TrNode.hasClass('active') == false) {
					TrNode.addClass('active');
					TrNode.children().last().html('<a href="javascript:;" _id="' + id + '" class="table-option-btn" onclick="getDeliveryList(' + id + "," + parentId + ')">收起列表</a>')
					$.ajax({
						type: "POST",
						url: '/v1.2/Bhdelivery/getDeliveryList',
						data:{'bh_campaign_id':id},
						dataType: 'json',
						success: function (rs) {
							tipTopHide();       //隐藏提示框
							console.log(rs)
							if (rs.api_code == 1) {         //如果查询成功
								var data = rs.data;     //返回数据赋值
								if (data.length == 0) {
									tipTopShow('暂无投放记录！');
									TrNode.removeClass('active');
									TrNode.children().last().html('<a href="javascript:;" _id="' + id + '" class="table-option-btn" onclick="getDeliveryList(' + id + "," + parentId +')">查看列表</a>')
									return;
								} else {
									renderadList1(data, TrNode);     //渲染表格数据		
								}
							} else {
								tipTopShow(rs.msg);
							}
						},
						error: function (rs) {
							tipTopShow("查询信息失败，请重试...");    //显示提示框
						}
					});
				} else {
					TrNode.removeClass('active');
					TrNode.next('.inner_table').remove();
					TrNode.children().last().html('<a href="javascript:;" _id="' + id + '" class="table-option-btn" onclick="getDeliveryList(' + id + "," + parentId +')">查看列表</a>');
				}
			}
			
			//渲染广告投放（二级）
			function renderadList(data, TrNode) {
				var str = "",
					table1 = '<tr class="inner_table" id="1"><td colspan="11" style="padding-right:0px !important;"><div class="inner_list" id="inner_list" style="margin-left:20px"><div class="tab_tip" style="height:40px;">'
				table1 += '<i></i><p>此推广计划共包括'+data.length+'条广告。 </p></div><table width="100%"><thead>'
