			//跳转至图表详情页面
			function ChartsDetails(id,type) {
				location.href = '/v1.2/report/info?id=' + id+'&pageType='+type;
			}

			//分页
			function renderPage(pages, curr) {
				//显示分页
				laypage({
					cont: $('#list_page'), //容器。值支持id名、原生dom对象，jquery对象。
					pages: pages, //通过后台拿到的总页数
					//skip: true, //是否开启跳页
					skin: '#4c94f5',
					prev: '<em class="layui-icon"></em>',
					next: '<em class="layui-icon"></em>',
					curr: curr || 1, //当前页
					jump: function (obj, first) { //触发分页后的回调
						if (!first) { //点击跳页触发函数自身，并传递当前页：obj.curr
							getList(obj.curr);
						}
					}
				});
			}
			
//			//全网分页
			function renderPage1(pages, curr) {
				//显示分页
				laypage({
					cont: $('#list_page'), //容器。值支持id名、原生dom对象，jquery对象。
					pages: pages, //通过后台拿到的总页数
					//skip: true, //是否开启跳页
					skin: '#4c94f5',
					prev: '<em class="layui-icon"></em>',
					next: '<em class="layui-icon"></em>',
					curr: curr || 1, //当前页
					jump: function (obj, first) { //触发分页后的回调
						if (!first) { //点击跳页触发函数自身，并传递当前页：obj.curr
							getCampaignList(obj.curr);
						}
					}
				});
			}
			getList();
			//广告列表展开
			$('body').on('click', '.view_list', function () {
				var id = $(this).attr('_id');
				$('#' + id).toggleClass('on');
			});

			//广告主详情
			$('body').on('click', '.view_info', function () {
				var id = $(this).attr('_id');
				console.log(id);
				var cacheDOM = $("#infoTemplate1").html();
				infocacheDOM = getUserInfo(id);
				laytpl(cacheDOM).render(infocacheDOM, function (result) {         //laytpl异步渲染调用
					//店铺详细信息弹框
					layer.open({
						type: 1,
						title: "广告详情",
						area: ['760px', '600px'],
						shadeClose: true,
						content: result
					});
				});
			});
			//全网广告详情弹出框
			$('body').on('click', '.view_info_bh', function () {
				var id = $(this).attr('_id');
				console.log(id);
				var cacheDOM = $("#infoTemplate2").html();
				infocacheDOM = getDeliveryInfo(id);
				laytpl(cacheDOM).render(infocacheDOM, function (result) {         //laytpl异步渲染调用
					//店铺详细信息弹框
					layer.open({
						type: 1,
						title: "广告详情",
						area: ['760px', '600px'],
						shadeClose: true,
						content: result
					});
				});
			});
			//审核失败理由
			$('body').on('click', '.status_tip', function () {
				layer.tips(this.getAttribute('data'), this);
			})
			//广告主详情数据
			function getUserInfo(id) {
				var data = '';
				$.ajax({
					type: "GET",
					url: '/v1.2/Gdtdelivery/deliveryInfo/delivery_id/' + id,
					dataType: 'json',
					async: false,
					beforeSend: function () {
						tipTopHide("正在查询数据");    //显示提示框
					},
					success: function (rs) {
						console.log(rs)
						tipTopHide();
						if (rs.state == 1) {         //如果查询成功
							data = rs.data;     //返回数据赋值
						} else {
							tipTopShow(rs.msg);
						}
					},
					error: function (rs) {
						console.log(rs)
						tipTopShow("查询信息失败，请重试...");    //显示提示框
					}
				});
				return data;
			}

			//全网广告详情
			function getDeliveryInfo(id){
				var data = '';
				$.ajax({
					type: "POST",
					url: '/v1.2/Bhdelivery/getDeliveryInfo',
					data:{'bh_delivery_id':id},
					dataType: 'json',
					async: false,
					beforeSend: function () {
						tipTopHide("正在查询数据");    //显示提示框
					},
					success: function (rs) {
						console.log(rs)
						tipTopHide();
						if (rs.api_code == 1) {         //如果查询成功
							data = rs.data;     //返回数据赋值
						} else {
							tipTopShow(rs.msg);
						}
					},
					error: function (rs) {
						console.log(rs)
						tipTopShow("查询信息失败，请重试...");    //显示提示框
					}
				});
				return data;
			}
			
			//保留两位小数（四舍五入）
			function round(number, fractionDigits) {
				with (Math) {
					return round(number * pow(10, fractionDigits)) / pow(10, fractionDigits);
				}
			}

		</script>
