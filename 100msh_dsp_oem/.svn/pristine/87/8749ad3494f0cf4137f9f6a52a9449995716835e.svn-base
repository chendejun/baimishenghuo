    .photos_div,
    .photosMore_div {
        width: 261px;
        height: 158px;
        border: 1px solid #dddddd;
        background: #f5f6f8;
        margin: 0 20px 20px 0;
        text-align: center;
        line-height: 19px;
        color: #999999;
        float: left;
        position: relative;
    }
    .photos_div{
        display: table;
    }

    .photos_div>span{
        display: table-cell;
        vertical-align: middle;
        text-align: center;
        padding: 15px;
    }
    .photosMore_div>span {
        padding: 15px;
        text-align: center;
        line-height: 128px;
        font-weight: 900;
    }
    .photos_div>span>h4 {
        font-weight: 900;
    }

    .pushImg,.pushmoreImg {
        width: 261px;
        height: 158px;
        position: absolute;
        left: 0;
        top: 0;
        opacity: 0;
    }
    .select_wrap {
        float: left;
        margin-right: 10px;
        position: relative;
    }

    .show {
        display: block !important;
    }

    .resultImg {
        width: 261px;
        height: 158px;
        display: block;
    }
